    "android": "ENVFILE=.env.staging react-native run-android",
    "ios": "ENVFILE=.env.staging react-native run-ios --simulator=\"iPhone 11 Pro\"",
    "start": "ENVFILE=.env.staging react-native start --reset-cache",
    "test": "ENVFILE=.env.test && jest",
    "coverage": "ENVFILE=.env.test jest --coverage",
