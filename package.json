{
  "name": "Pickyourtrail",
  "version": "2.0.5",
  "build": 297,
  "private": true,
  "scripts": {
    "pod-install": "cd ios && pod install && cd ..",
    "start": "ENVFILE=.env.staging react-native start --reset-cache",
    "android": "ENVFILE=.env.staging react-native run-android",
    "ios": "ENVFILE=.env.staging react-native run-ios --simulator=\"iPhone 11 Pro\"",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:report": "yarn test || concurrently \"http-server jest-report -p 9012\" \"open-cli http://localhost:9012/\"",
    "coverage": "jest --coverage",
    "coverage:report": "yarn coverage || concurrently \"http-server coverage -p 9010\" \"open-cli http://localhost:9010/\"",
    "lint": "eslint .",
    "lint:generate-report": "yarn lint -f node_modules/eslint-detailed-reporter/lib/detailed.js -o lint-report/index.html",
    "lint:report": "yarn lint:generate-report || concurrently \"http-server lint-report -p 9011\" \"open-cli http://localhost:9011/\"",
    "build-android": "yarn jetify && cd android && ./gradlew bundleRelease && cd ..",
    "build-android-alpha": "ENVFILE=.env.staging yarn build-android",
    "build-android-prod": "ENVFILE=.env.prod yarn build-android",
    "build-ios": "fastlane ios uploadToTestflight",
    "build-ios-testflight": "ENVFILE=.env.staging yarn build-ios",
    "build-ios-prod": "ENVFILE=.env.prod yarn build-ios",
    "alpha-android": "fastlane android alpha",
    "bump-version": "node bumpVersion.js",
    "commit": "git-cz",
    "android:beta": "cd android && fastlane android beta",
    "android:playstore": "cd android && fastlane android playstore",
    "ios:beta": "cd ios && fastlane ios beta",
    "ios:release": "cd ios && fastlane ios release"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
